alias av="less ~/.zsh/aliases"
alias fe="vi ~/.zsh/functions"
alias fv="less ~/.zsh/functions"

alias be="bundle exec"
alias b="bundle"
alias bu="bundle update"

alias ga="git add"
alias gA="git add -A"
alias gca="git commit --amend"
alias gc="git commit"
alias gco="git checkout"
alias gd="git diff-base"
alias gda="git diff-base HEAD"
alias gds="git diff-base --cached"
alias gl="git log"
alias gps="git push"
alias gpsf="git push --force"
alias gpl="git pull"
alias gr="git rebase"
alias gri="git rebase -i"
alias grc="git rebase --continue"
alias grs="git rebase --skip"
alias gra="git rebase --abort"
alias grh="git reset HEAD"
alias gun="git reset HEAD^1"

