{
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json",
    "ruby",
    "rust",
    "css",
    "scss"
  ],
  "css.validate": false,
  "coc.preferences.formatOnType": true,
  "coc.preferences.jumpCommand": "tabe",
  "diagnostic.checkCurrentLine": true,
  "diagnostic.enableMessage": "jump",
  "diagnostic.floatConfig.maxWidth": 200,
  "diagnostic.floatConfig.maxHeight": 1500,
  "diagnostic.messageDelay": 1000,
  "diagnostic.refreshOnInsertMode": true,
  "eslint.autoFixOnSave": true,
  "eslint.fixOnSaveTimeout": 10000,
  "eslint.format.enable": true,
  "hover.floatConfig.maxWidth": 200,
  "hover.floatConfig.maxHeight": 1500,
  "javascript.suggest.enabled": false,
  "javascript.suggest.completeFunctionCalls": false,
  "javascript.suggestionActions.enabled": false,
  "javascript.validate.enable": true,
  "json.format.enable": true,
  "less.validate": false,
  "prettier.disableSuccessMessage": true,
  "scss.validate": false,
  "signature.enable": false,
  "stylelintplus.autoFixOnSave": true,
  "suggest.autoTrigger": "none",
  "suggest.floatEnable": false,
  "tsserver.enableJavascript": true,
  "tsserver.formatOnType": false,
  "tsserver.log": "verbose",
  "tsserver.trace.server": "verbose",
  "typescript.suggest.enabled": false,
  "typescript.suggest.completeFunctionCalls": false,
  "typescript.suggestionActions.enabled": false,
  "wxss.validate": false
}
